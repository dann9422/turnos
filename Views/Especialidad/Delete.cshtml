@model turnos.Models.especialidad
<div class="container">
<h3> Estas seguro de eliminar el siguinete registro?</h3>
<div class="row">
        <div class="col s12 m6">
      <div class="card hoverable teal accent-4">
        <div class="card-content white-text">
        <span class="card-title"><h5><b> @Html.DisplayFor(model => model.descripcion)</b></h5></span>
    <p>
        <dt>
            @Html.DisplayNameFor(Modelo=> Modelo.descripcion):  @Html.DisplayFor(modelo => modelo.descripcion)
        </dt>
    </p>
 
        </div>
         <div class="card-action blue-grey darken-2" >
<form asp-action="Delete" >
<input type="hidden" asp-for="idEspecialidad"/>
<input type="submit" value="Eliminar" class="waves-effect waves-light btn-small orange lighten-2" >
        <a class="right" asp-action="Index">Volver</a>
    </form>
    </div>
    <div>
    </div>
</div>
</div>