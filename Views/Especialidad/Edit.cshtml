@model turnos.Models.especialidad

<div class="container">
<h4>Especialidad</h4>
<div class="row">
    <form asp-action="Edit">
        <div asp-validation-summary="ModelOnly"></div>
        <input type="hidden" asp-for="idEspecialidad"/>
           <div class="row">
        <div class="input-field col s6">
          <i class="material-icons prefix">add_to_queue</i>
          <label asp-for="descripcion"></label>
            <input asp-for="descripcion"/>
            <span asp-validation-for="descripcion"></span>
        </div>
         </div>
         <div class="row" >
            <div class="col s6 m6 ">
                <button class="btn waves-effect waves-light cyan accent-4" type="submit" name="action">Guardar
                 <i class="material-icons right">send</i>
                        </button>
        
            </div>
            <div class="col s6 m6">
                <a class="waves-effect waves-light btn grey darken-4" asp-action="Index"><i class="material-icons left ">undo</i>volver</a>
            </div>
            </div>
       
    </form>
    </div>
</div>
